<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeActivity">


    <ImageView
        android:id="@+id/imageView"
        android:layout_marginStart="0dp"
        android:layout_marginTop="-8dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/bigger_rectangle"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="13dp" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="120dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/baseline_square_24" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="120dp"
        android:layout_marginStart="0dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/user_profile_empty_foreground"/>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="85dp"
        android:layout_marginBottom="90dp"
        android:layout_centerHorizontal="true"
        android:text="Profil"
        android:textColor="@color/white"
        android:textSize="32dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView1_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="70dp"
        android:layout_marginTop="270dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="90dp"
        android:text="Nama Pengguna"
        android:textAlignment="center"
        android:textSize="16sp"
        />

    <EditText
        android:id="@+id/editTextTextMultiLine1_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="290dp"
        android:layout_marginStart="70dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine" />

    <TextView
        android:id="@+id/textView2_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="70dp"
        android:layout_marginTop="350dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="90dp"
        android:text="Email"
        android:textAlignment="center"
        android:textSize="16sp"
        />

    <EditText
        android:id="@+id/editTextTextMultiLine2_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="370dp"
        android:layout_marginStart="70dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine" />

    <TextView
        android:id="@+id/textView3_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="70dp"
        android:layout_marginTop="430dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="90dp"
        android:text="Nomor Telepon"
        android:textAlignment="center"
        android:textSize="16sp"
        />

    <EditText
        android:id="@+id/editTextTextMultiLine3_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="450dp"
        android:layout_marginStart="70dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine" />

    <TextView
        android:id="@+id/textView4_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="70dp"
        android:layout_marginTop="510dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="90dp"
        android:text="Lokasi"
        android:textAlignment="center"
        android:textSize="16sp"
        />

    <EditText
        android:id="@+id/editTextTextMultiLine4_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="530dp"
        android:layout_marginStart="70dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine" />

    <Button
        android:id="@+id/buttonAjukan"
        style="@style/Widget.Material3.Button"
        android:layout_width="242dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="85dp"
        android:layout_marginTop="600dp"
        android:layout_marginEnd="85dp"
        android:layout_marginBottom="0dp"
        android:backgroundTint="#4EB193"
        android:text="Ajukan profil Penjual" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="650dp"
        android:outlineAmbientShadowColor="@color/design_default_color_primary_dark"
        android:outlineSpotShadowColor="@color/black"
        android:backgroundTint="@color/cardview_shadow_start_color"

        android:src="@drawable/rectangle_white"/>

    <ImageButton
        android:id="@+id/buttonHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="680dp"
        android:background="@color/material_on_primary_disabled"
        android:src="@drawable/home_unclick2" />

    <ImageButton
        android:id="@+id/buttonUpload"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginTop="680dp"
        android:background="@color/material_on_primary_disabled"
        android:src="@drawable/paper_upload" />

    <ImageButton
        android:id="@+id/buttonBuy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/material_on_primary_disabled"
        android:layout_marginTop="680dp"
        android:layout_marginStart="235dp"
        android:src="@drawable/buy"/>

    <ImageButton
        android:id="@+id/buttonProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/material_on_primary_disabled"
        android:layout_marginTop="680dp"
        android:layout_marginStart="340dp"
        android:src="@drawable/profile_clicked"/>




</RelativeLayout>